{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "echo 'USING UI INSTALL OVERRIDE' && node -v && npx -y pnpm@9.12.1 -v && npx -y pnpm@9.12.1 install",
  "buildCommand": "npx -y pnpm@9.12.1 --filter raditail run build-storybook",
  "outputDirectory": "packages/raditail/storybook-static",
  "env": {
    "HUSKY": "0",
    "CI": "true",
    "COREPACK_ENABLE_DOWNLOAD_PROMPT": "0",
    "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/"
  }
}
